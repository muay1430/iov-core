{
  "name": "@iov/tendermint-rpc",
  "version": "0.4.1",
  "description": "Codec to encode/decode bns transactions and state objects",
  "author": "IOV SAS <admin@iov.one>",
  "license": "Apache-2.0",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/iov-one/iov-core/tree/master/packages/iov-tendermint-rpc"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "docs": "rm -rf docs && typedoc --options typedoc.js",
    "lint": "tslint -t verbose --project . ${TSLINT_FLAGS}",
    "format": "prettier --write --loglevel warn \"./src/**/*.ts\"",
    "test-node": "node jasmine-testrunner.js",
    "test-edge": "yarn pack-web && karma start --single-run --browsers Edge",
    "test-firefox": "yarn pack-web && karma start --single-run --browsers Firefox",
    "test-chrome": "yarn pack-web && karma start --single-run --browsers ChromeHeadless",
    "test-safari": "yarn pack-web && karma start --single-run --browsers Safari",
    "test": "yarn build-or-skip && yarn test-node",
    "prebuild": "yarn format && yarn lint",
    "build": "rm -rf build && tsc",
    "build-or-skip": "[ -n \"$SKIP_BUILD\" ] || yarn build",
    "pack-web": "yarn build-or-skip && webpack --mode development --config webpack.web.config.js"
  },
  "dependencies": {
    "@iov/encoding": "^0.4.0",
    "@iov/tendermint-types": "^0.4.0",
    "@types/node": "^10.3.2",
    "@types/ws": "^5.1.2",
    "axios": "^0.18.0",
    "events": "^3.0.0",
    "isomorphic-ws": "^4.0.1",
    "readonly-date": "^1.0.0",
    "type-tagger": "^1.0.0",
    "ws": "^5.2.2",
    "xstream": "^11.7.0"
  },
  "gitHead": "39379b38f0ac128a48aa7174692c4c932ed326f7"
}
